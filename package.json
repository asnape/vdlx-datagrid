{
  "name": "@fico-xpress/vdlx-datagrid",
  "version": "4.6.2",
  "description": "",
  "scripts": {
    "postinstall": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "watch": "NODE_ENV=development parcel watch --no-source-maps -d insight/client_resources/vdlx-datagrid -o vdlx-datagrid.js --no-hmr src/js/index.js",
    "copy-production-to-app": "copyfiles -f \"dist/insight/client_resources/vdlx-datagrid/*\" insight/client_resources/vdlx-datagrid",
    "concat-header-js": "concat src/build/license-header.txt dist/insight/client_resources/vdlx-datagrid/vdlx-datagrid.js -o dist/insight/client_resources/vdlx-datagrid/vdlx-datagrid.js",
    "concat-header-css": "concat src/build/license-header.txt dist/insight/client_resources/vdlx-datagrid/vdlx-datagrid.css -o dist/insight/client_resources/vdlx-datagrid/vdlx-datagrid.css",
    "prebuild": "npm run clean",
    "clean": "rimraf dist/insight/client_resources/vdlx-datagrid/*.js dist/insight/client_resources/vdlx-datagrid/*.css dist/insight/client_resources/vdlx-datagrid-debug/*.js dist/insight/client_resources/vdlx-datagrid-debug/*.css insight/client_resources/vdlx-datagrid dist/insight/client_resources/*.zip insight/model_resources/*.csv *.zip",
    "build": "parcel build --no-cache --no-source-maps -d dist/insight/client_resources/vdlx-datagrid -o vdlx-datagrid.js src/js/index.js",
    "postbuild": "npm run concat-header-js && npm run concat-header-css && npm run copy-production-to-app",
    "build:debug": "NODE_ENV=development parcel build --no-cache --no-minify --no-source-maps -d dist/insight/client_resources/vdlx-datagrid-debug -o vdlx-datagrid-debug.js src/js/index.js",
    "bundle:library": "cd dist/insight/client_resources/ && bestzip vdlx-datagrid.zip vdlx-datagrid/",
    "bundle:debug-library": "cd dist/insight/client_resources/ && bestzip vdlx-datagrid-debug.zip vdlx-datagrid-debug/",
    "bundle:app": "cd insight && bestzip ../vdlx-datagrid-examples-app.zip *",
    "generate-test-data": "node generate-data.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@types/bootbox": "^4.4.36",
    "@types/jest": "^24.9.0",
    "@types/jquery": "^3.3.31",
    "@types/knockout": "^3.4.60",
    "@types/lodash": "^4.14.146",
    "@types/tabulator-tables": "^4.4.5",
    "jest": "^24.9.0",
    "babel-jest": "^24.9.0"
  },
  "dependencies": {
    "autoprefixer": "^9.7.1",
    "babel-core": "7.0.0-bridge.0",
    "bestzip": "^2.1.5",
    "concat": "^1.0.3",
    "copyfiles": "^2.1.1",
    "csv-stringify": "^4.3.1",
    "knockout": "^3.5.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "rimraf": "^3.0.0",
    "sass": "^1.23.3",
    "lodash-es": "^4.17.15",
    "tabulator-tables": "^4.4.3"
  },
  "browserslist": {
    "production": [
      "IE 11"
    ],
    "development": [
      "last 2 chrome versions"
    ]
  }
}
