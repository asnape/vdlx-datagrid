{
  "name": "@fico-xpress/vdlx-datagrid",
  "version": "4.6.1",
  "description": "",
  "scripts": {
    "postinstall": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "watch": "parcel watch -d insight/client_resources/vdlx-datagrid -o vdlx-datagrid.js --no-hmr src/js/index.js",
    "copy-production-to-app": "copyfiles -f \"dist/insight/client_resources/vdlx-datagrid/*\" insight/client_resources/vdlx-datagrid",
    "build": "parcel build -d dist/insight/client_resources/vdlx-datagrid -o vdlx-datagrid.js src/js/index.js && npm run copy-production-to-app",
    "bundle:library": "cd dist/insight/client_resources/ && bestzip vdlx-datagrid.zip vdlx-datagrid/",
    "bundle:app": "cd insight && bestzip ../vdlx-datagrid-examples-app.zip *",
    "generate-test-data": "node generate-data.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@types/bootbox": "^4.4.36",
    "@types/jest": "^24.9.0",
    "@types/jquery": "^3.3.31",
    "@types/knockout": "^3.4.60",
    "@types/lodash": "^4.14.146",
    "@types/tabulator-tables": "^4.4.5",
    "autoprefixer": "^9.7.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "bestzip": "^2.1.5",
    "copyfiles": "^2.1.1",
    "csv-stringify": "^4.3.1",
    "jest": "^24.9.0",
    "knockout": "^3.5.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "sass": "^1.23.3",
    "lodash-es": "^4.17.15",
    "tabulator-tables": "^4.4.3"
  }
}
